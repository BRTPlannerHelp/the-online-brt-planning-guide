{
	"prepare" : {
		"dockerfile" : {
			"type" : "path",
			"data" : ".robrt.Dockerfile"
		}
	},
	"build" : {
		"cmds" : [
			"cd $ROBRT_REPOSITORY_DIR",
			"cd generator",
			"haxe test.hxml",
			"haxe build.hxml",
			"cd $ROBRT_REPOSITORY_DIR",
			"cp -r docs/design/web/mockup $ROBRT_OUTPUT_DIR/web-mockup"
		]
	}
}

